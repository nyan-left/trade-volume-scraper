{
  "name": "osrs-trade-stats",
  "version": "0.0.0",
  "description": "A simple, typed scraper of Old School RuneScape grand exchange. Includes daily trade volume data.",
  "engines": {
    "node": ">= 14.15 <15"
  },
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "~6.15.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "tsutils": "^3.17.1",
    "typescript": "^4.1.2",
    "ts-mocha": "^8.0.0"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint ./src --ext .ts,.tsx",
    "test": "ts-mocha -p tsconfig.json tests/test.ts --timeout 10000"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "build/**/*"
  ],
  "author": "nyan-left",
  "license": "MIT",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.14",
    "assert": "^2.0.0",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "moment": "^2.29.1",
    "tslib": "^2.0.3",
    "user-agents": "^1.0.559"
  },
  "volta": {
    "node": "14.15.0"
  }
}
